{
  "$schema": "http://json-schema.org/schema#",
  "type": "object",
  "properties": {
    "replicaCount": {
      "type": "integer",
      "title": "副本数量",
      "description": "要部署的副本数量",
      "minimum": 1,
      "default": 1
    },
    "resources": {
      "type": "object",
      "title": "资源需求",
      "properties": {
        "limits": {
          "type": "object",
          "title": "资源限制",
          "properties": {
            "cpu": {
              "type": "string",
              "title": "CPU限制",
              "pattern": "^[0-9]+m?$",
              "default": "1000m"
            },
            "memory": {
              "type": "string",
              "title": "内存限制",
              "pattern": "^[0-9]+(Mi|Gi)$",
              "default": "1024Mi"
            }
          }
        },
        "requests": {
          "type": "object",
          "title": "资源请求",
          "properties": {
            "cpu": {
              "type": "string",
              "title": "CPU请求",
              "pattern": "^[0-9]+m?$",
              "default": "500m"
            },
            "memory": {
              "type": "string",
              "title": "内存请求",
              "pattern": "^[0-9]+(Mi|Gi)$",
              "default": "512Mi"
            }
          }
        }
      }
    },
    "persistence": {
      "type": "object",
      "title": "持久化配置",
      "properties": {
        "enabled": {
          "type": "boolean",
          "title": "启用持久化",
          "default": true
        },
        "accessMode": {
          "type": "string",
          "title": "访问模式",
          "enum": [
            "ReadWriteOnce",
            "ReadOnlyMany",
            "ReadWriteMany"
          ],
          "default": "ReadWriteOnce"
        },
        "size": {
          "type": "string",
          "title": "存储大小",
          "pattern": "^[0-9]+(Mi|Gi)$",
          "default": "1Gi"
        },
        "storageClass": {
          "type": "string",
          "title": "存储类",
          "default": "local"
        }
      }
    },
    "env": {
      "type": "object",
      "title": "环境变量",
      "properties": {
        "env1": {
          "type": "object",
          "title": "环境变量1",
          "properties": {
            "name": {
              "type": "string",
              "title": "变量名称",
              "pattern": "^[A-Z][A-Z0-9_]*$",
              "default": "DB_HOST"
            },
            "value": {
              "type": "string",
              "title": "变量值",
              "default": "mysql"
            }
          },
          "required": [
            "name",
            "value"
          ]
        },
        "env2": {
          "type": "object",
          "title": "环境变量2",
          "properties": {
            "name": {
              "type": "string",
              "title": "变量名称",
              "pattern": "^[A-Z][A-Z0-9_]*$",
              "default": "DB_PORT"
            },
            "value": {
              "type": "string",
              "title": "变量值",
              "default": "3306"
            }
          },
          "required": [
            "name",
            "value"
          ]
        },
        "env3": {
          "type": "object",
          "title": "环境变量3",
          "properties": {
            "name": {
              "type": "string",
              "title": "变量名称",
              "pattern": "^[A-Z][A-Z0-9_]*$",
              "default": "DB_DATABASE"
            },
            "value": {
              "type": "string",
              "title": "变量值",
              "default": "lsky"
            }
          },
          "required": [
            "name",
            "value"
          ]
        },
        "env4": {
          "type": "object",
          "title": "环境变量4",
          "properties": {
            "name": {
              "type": "string",
              "title": "变量名称",
              "pattern": "^[A-Z][A-Z0-9_]*$",
              "default": "DB_USERNAME"
            },
            "value": {
              "type": "string",
              "title": "变量值",
              "default": "lsky"
            }
          },
          "required": [
            "name",
            "value"
          ]
        },
        "env5": {
          "type": "object",
          "title": "环境变量5",
          "properties": {
            "name": {
              "type": "string",
              "title": "变量名称",
              "pattern": "^[A-Z][A-Z0-9_]*$",
              "default": "DB_PASSWORD"
            },
            "value": {
              "type": "string",
              "title": "变量值",
              "default": "lsky_password"
            }
          },
          "required": [
            "name",
            "value"
          ]
        },
        "env6": {
          "type": "object",
          "title": "环境变量6",
          "properties": {
            "name": {
              "type": "string",
              "title": "变量名称",
              "pattern": "^[A-Z][A-Z0-9_]*$",
              "default": "APP_URL"
            },
            "value": {
              "type": "string",
              "title": "变量值",
              "default": "http://localhost"
            }
          },
          "required": [
            "name",
            "value"
          ]
        },
        "env7": {
          "type": "object",
          "title": "环境变量7",
          "properties": {
            "name": {
              "type": "string",
              "title": "变量名称",
              "pattern": "^[A-Z][A-Z0-9_]*$",
              "default": "APP_KEY"
            },
            "value": {
              "type": "string",
              "title": "变量值",
              "default": "base64:random_string_here"
            }
          },
          "required": [
            "name",
            "value"
          ]
        },
        "env8": {
          "type": "object",
          "title": "环境变量8",
          "properties": {
            "name": {
              "type": "string",
              "title": "变量名称",
              "pattern": "^[A-Z][A-Z0-9_]*$",
              "default": "APP_DEBUG"
            },
            "value": {
              "type": "string",
              "title": "变量值",
              "default": "false"
            }
          },
          "required": [
            "name",
            "value"
          ]
        },
        "env9": {
          "type": "object",
          "title": "环境变量9",
          "properties": {
            "name": {
              "type": "string",
              "title": "变量名称",
              "pattern": "^[A-Z][A-Z0-9_]*$",
              "default": "REDIS_HOST"
            },
            "value": {
              "type": "string",
              "title": "变量值",
              "default": "redis"
            }
          },
          "required": [
            "name",
            "value"
          ]
        },
        "env10": {
          "type": "object",
          "title": "环境变量10",
          "properties": {
            "name": {
              "type": "string",
              "title": "变量名称",
              "pattern": "^[A-Z][A-Z0-9_]*$",
              "default": "REDIS_PASSWORD"
            },
            "value": {
              "type": "string",
              "title": "变量值",
              "default": "null"
            }
          },
          "required": [
            "name",
            "value"
          ]
        },
        "env11": {
          "type": "object",
          "title": "环境变量11",
          "properties": {
            "name": {
              "type": "string",
              "title": "变量名称",
              "pattern": "^[A-Z][A-Z0-9_]*$",
              "default": "REDIS_PORT"
            },
            "value": {
              "type": "string",
              "title": "变量值",
              "default": "6379"
            }
          },
          "required": [
            "name",
            "value"
          ]
        }
      }
    }
  },
  "required": [
    "replicaCount",
    "resources",
    "persistence"
  ]
}